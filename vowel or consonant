import java.util.*;
class vowel
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        char a=sc.nextLine().charAt(0);
        char c=Character.toLowerCase(a);
        switch(c)
        {
            case 'a':
            case 'e':
            case 'i':
            case 'o':
            case 'u':
                System.out.print("Vowel");
                break;
            default:
                System.out.print("Consonant");
        }
    }
}
