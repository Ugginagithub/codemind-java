import java.util.*;
class ps
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n;
        n=sc.nextInt();
        for(int j=0;j<n;j++)
        {
            int a=sc.nextInt();
            int c=0;
            for(int i=0;i<a;i++)
            {
                if((i*i)==a)
                {
                    c+=1;
                }
            }
            if(c==1)
            {
                System.out.println("True");
            }
            else
            {
                System.out.println("False");
            }
        }
    }
}
