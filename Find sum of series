import java.util.*;
class sum
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,i;
        double c=0.0;
        a=sc.nextInt();
        for(i=1;i<=a;i++)
        {
            c+=(1.0/i);
        }
        System.out.format("%.2f",c);
    }
}
