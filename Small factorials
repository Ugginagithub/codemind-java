import java.util.*;
class fact
{
    public static int fact(int n)
    {
        int f=1;
        for(int i=2;i<=n;i++)
        {
            f*=i;
        }
        return f;
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        while(n-->0)
        {
            int a=sc.nextInt();
            System.out.format("%d\n",fact(a));
        }
    }
}
